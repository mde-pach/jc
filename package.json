{
  "name": "jc",
  "version": "0.1.0",
  "description": "Auto-discovery component showcase with TypeScript props introspection and faker integration",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./advanced": {
      "import": "./dist/advanced.js",
      "types": "./dist/advanced.d.ts"
    },
    "./config": {
      "import": "./dist/config.js",
      "types": "./dist/config.d.ts"
    },
    "./next": {
      "import": "./dist/next.js",
      "types": "./dist/next.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./plugins/lucide": {
      "import": "./dist/plugins/lucide.js",
      "types": "./dist/plugins/lucide.d.ts"
    }
  },
  "bin": {
    "jc": "./dist/cli.cjs"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "prepublishOnly": "bun run type-check && bun run test && bun run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "lucide-react": ">=0.300.0",
    "typescript": ">=5"
  },
  "peerDependenciesMeta": {
    "lucide-react": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^28.1.0",
    "react-docgen-typescript": "^2.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "components",
    "showcase",
    "storybook-alternative",
    "typescript",
    "props-introspection",
    "react",
    "component-library",
    "playground",
    "zero-config",
    "documentation",
    "developer-tools"
  ],
  "license": "MIT",
  "author": "Maxime de Pachtere",
  "homepage": "https://github.com/mde-pach/jc#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mde-pach/jc.git"
  }
}
